<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane minHeight="720.0" minWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.d19.teamD.UIControllerVU">
   <children>
      <ImageView fx:id="backgroundImage" opacity="0.6" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@edu/wpi/cs3733/d19/teamD/resources/Hospitalbackground.png" />
         </image>
      </ImageView>
      <BorderPane>
         <center>
            <TableView fx:id="usersTable" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="130.0" text="edu.wpi.cs3733.d19.teamD.User ID" />
                <TableColumn prefWidth="130.0" text="Permissions" />
                  <TableColumn prefWidth="100.0" text="Username" />
                  <TableColumn prefWidth="100.0" text="Remove" />
              </columns>
               <styleClass>
                  <String fx:value="table-view" />
                  <String fx:value="table-row-cell" />
               </styleClass>
            </TableView>
         </center>
      </BorderPane>
   </children>
</StackPane>
